{
  "name": "nemo wallet",
  "description": "Transfer funds to any wallet",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "fish.png"
  },
  "icons": {
    "16": "fish.png",
    "32": "fish.png",
    "48": "fish.png",
    "128": "fish.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "unlimitedStorage",
    "nativeMessaging"
  ]
}
